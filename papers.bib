@Article{         schuett08,
  abstract      = {We determine all complex K3 surfaces with Picard rank 20
                  over Q. Here the Neron-Severi group has rank 20 and is
                  generated by divisors which are defined over Q. Our proof
                  uses modularity, the Artin-Tate conjecture and class group
                  theory. With different techniques, the result has been
                  established by Elkies to show that Mordell-Weil rank 18
                  over Q is impossible for an elliptic K3 surface. We then
                  apply our methods to general singular K3 surfaces, i.e.
                  with Neron-Severi group of rank 20, but not necessarily
                  generated by divisors over Q.},
  author        = {Matthias Schuett},
  eprint        = {0804.1558v3},
  month         = {04},
  title         = {K3 surfaces with Picard rank 20},
  year          = {2008}
}

@Article{         schurg11,
  abstract      = {Let S be a projective algebraic complex K3-surface, g a
                  nonnegative integer, and $\beta \neq 0$ in
                  $H_{2}(S,\mathbb{Z})$ a curve class. We construct a derived
                  stack
                  $\mathbb{R}\bar{\mathbf{M}}^{\textrm{red}}_{g}(S;\beta)$
                  whose truncation is the usual stack
                  $\bar{\mathbf{M}}_{g}(S;\beta)$ of stable maps from curves
                  of genus g to S hitting the class \beta, and such that the
                  inclusion $\bar{\mathbf{M}}_{g}(S;\beta)\hookrightarrow
                  \mathbb{R}\bar{\mathbf{M}}^{\textrm{red}}_{g}(S;\beta)$
                  induces on $\bar{\mathbf{M}}_{g}(S;\beta)$ a perfect
                  obstruction theory whose tangent and obstruction spaces
                  coincide with the corresponding reduced spaces of
                  Okounkov-Maulik-Pandharipande-Thomas. The approach we
                  present here yields not only a full rigorous proof of the
                  existence of a reduced obstruction theory, not relying on
                  any result on semiregularity maps, but also a new global
                  geometric interpretation. An important ingredient of our
                  construction is a perfect determinant map from the stack of
                  perfect complexes to the stack of line bundles whose
                  tangent morphism is Illusie's trace map for perfect
                  complexes. We expect that this determinant map might be
                  useful in other contexts as well.},
  author        = {Timo Sch{\"u}rg and Bertrand To{\"e}n and Gabriele Vezzosi},
  eprint        = {1102.1150v1},
  month         = {02},
  title         = {Reduced obstruction theory for the stack of stable maps to
                  a K3-surface via derived algebraic geometry},
  year          = {2011}
}

